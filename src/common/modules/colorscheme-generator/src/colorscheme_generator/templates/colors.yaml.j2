# Color scheme generated from: {{ source_image }}
# Backend: {{ backend }}
# Generated: {{ generated_at }}

metadata:
  source_image: "{{ source_image }}"
  backend: "{{ backend }}"
  generated_at: "{{ generated_at }}"

special:
  background: "{{ background.hex }}"
  foreground: "{{ foreground.hex }}"
  cursor: "{{ cursor.hex }}"

colors:
{% for color in colors %}  - "{{ color.hex }}"
{% endfor %}

rgb:
  background: [{{ background.rgb[0] }}, {{ background.rgb[1] }}, {{ background.rgb[2] }}]
  foreground: [{{ foreground.rgb[0] }}, {{ foreground.rgb[1] }}, {{ foreground.rgb[2] }}]
  cursor: [{{ cursor.rgb[0] }}, {{ cursor.rgb[1] }}, {{ cursor.rgb[2] }}]
  colors:
{% for color in colors %}    - [{{ color.rgb[0] }}, {{ color.rgb[1] }}, {{ color.rgb[2] }}]
{% endfor %}
